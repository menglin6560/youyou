<!--pages/mymovie/mymovie.wxml-->
<view class="movielist">
</view>
<view class="upload">
  <!--vant 输入框-->
   <van-field
    value="{{ content }}"
    placeholder="写下你的想法"
    bind:change="onContentChange"
  />
  <!--vant 按钮上传图片-->
  <van-button type="primary" size="large" bindtap="upload" custom-class="btn-upload">上传图片</van-button>
  <!--vant 提交按钮-->
  <van-button  type="danger" size="large" bindtap="submit">提交</van-button>  
</view>
